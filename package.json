{
  "name": "clinica-ballarin-dashboard",
  "version": "1.0.0",
  "description": "Sistema de Controle de Estoque e Dashboard Analytics para ClÃ­nica Ballarin",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "ts-node --transpile-only src/server.ts",
    "client:dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "start": "node dist/server.js",
    "setup": "echo 'Supabase Cloud - No local setup required'",
    "test-api": "node test-api.js",
    "clean": "rm -rf dist node_modules package-lock.json",
    "health-check": "curl http://localhost:3000/api/health"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@supabase/supabase-js": "^2.39.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.4.5",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "ts-node": "^10.9.1",
    "vite": "^4.4.5",
    "concurrently": "^8.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24"
  },
  "keywords": [
    "react",
    "typescript",
    "express",
    "supabase",
    "dashboard",
    "analytics",
    "inventory",
    "clinic"
  ],
